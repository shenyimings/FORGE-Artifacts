        vm.stopPrank();

        vm.roll(9962945);

        vm.startPrank(owner);

        p2pSsvProxyFactory.depositEthAndRegisterValidators{value: 64 ether}(
            getDepositData2(),
            address(0x548D1cA3470Cf9Daa1Ea6b4eF82A382cc3e24c4f),
